//exercise 4 - prime numbers
println("Please enter a number: ");
int max = Integer.parseInt(System.console().readLine());
int test = 2;
int result = 1;
while (test<max){
	if (max%test==0){
		println(max + " is not a prime number");
		result = 0;
		break;
		}
	else{
		test = test + 1;
		}
	}
	
if (result==1){
	println(max + " is a prime number");
	}