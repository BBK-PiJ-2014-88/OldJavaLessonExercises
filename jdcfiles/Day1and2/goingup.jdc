//Exercise 9 - Going up
int compare = 0;
String result = "Yes";


println("Please enter a number: ");
int input = Integer.parseInt(System.console().readLine());
if(input==-1){
print("No numbers were entered");
}



while(true){
compare = Integer.parseInt(System.console().readLine());
if (compare==-1){
break;
}
else if(compare!= input+1){
result = "No";
}

input = compare;

 
}



println(result);